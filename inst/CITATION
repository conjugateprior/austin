
citHeader("To cite austin in publications use:")

citEntry(entry="Manual",
         title = "Austin: Do Things with Words",
         author = "Will Lowe",
         year   = 2021,
         url    = "http://conjugateprior.github.io/austin",
         textVersion = 
         paste("Will Lowe 2021.", 
               "Austin: Do things with words. Version 0.5.0",
               "URL http://conjugateprior.github.io/austin")
         )

